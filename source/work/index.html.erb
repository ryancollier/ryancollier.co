<% content_for :page_js do %>
  
  <script type="text/javascript">

    $(document).ready(function() {
      //$("a.fancybox").fancybox({'type' : 'image'});

    $(".fancybox")
    .fancybox({
        type: 'image',
        helpers : {
          title: {
            type: 'inside',
            position: 'bottom'
          }
        }, 
        beforeLoad: function() {
            var el, id = $(this.element).data('title-id');
            if (id) {
                el = $('#' + id);
                if (el.length) {
                    this.title = el.html();
                }
            }
        }
    });

    });

  </script>

